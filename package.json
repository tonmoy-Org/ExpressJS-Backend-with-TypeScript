{
  "name": "simple-crud-backend",
  "version": "1.0.0",
  "description": "A simple CRUD backend using Express.js, TypeScript, Mongoose, and Redis.",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node-dev src/server.ts",
    "build": "tsc",
    "serve": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "mongoose",
    "redis",
    "crud",
    "backend"
